pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
--adventure game
--by pi_of_the_beholder

-- main
state = "play" -- or "win", "lose", "inventory", etc.

function _init()
    map_setup()
    make_player()
    --make_enemy()
end

function _update()
    if state == "play" then
        update_map()
        move_player()
        check_win_lose()
    elseif state == "win" or state == "lose" then
        if btnp(❎) then
            extcmd("reset")
        end
    end
end

function _draw()
	cls()
	
	if state == "play" then
		draw_map()
		draw_player(p.sprite)
		--draw_enemy(e.sprite)
		if (btn(❎)) show_inventory()
	else
		draw_win_lose()
	end
end
-->8
-- map

function map_setup()
    --timers
    timer=0
    anim_time=30 --30 = 1 second

    --map tile settings
    wall=0
    key=1
    door=2
    anim1=3
    anim2=4
    chest=5
    lose=6
    win=7
end

function update_map()
    if (timer<0) then
        toggle_tiles()
        timer=anim_time
    end
    timer-=1
end

function draw_map()
    mapx=flr(p.x/16)*16
    mapy=flr(p.y/16)*16
    camera(mapx*8,mapy*8)

    map(0,0,0,0,128,64)
end

function is_tile(tile_type,x,y)
    tile=mget(x,y)
    has_flag=fget(tile,tile_type)
    return has_flag
end

function can_move(x,y)
    return not is_tile(wall,x,y)
end

function swap_tile(x,y)
    tile=mget(x,y)
    mset(x,y,tile+1)
end

function unswap_tile(x,y)
    tile=mget(x,y)
    mset(x,y,tile-1)
end

function get_key(x,y)
    p.keys+=1
    swap_tile(x,y)
    sfx(1)
end

function open_door(x,y)
	p.keys-=1
	swap_tile(x,y)
	sfx(2)
end

function open_chest(x,y)
	p.keys-=1
	swap_tile(x,y)
	sfx(1)
end
-->8
-- player

function make_player()
    p={}
    p.x=3
    p.y=3
    p.sprite=1
    p.keys=0
    p.gold=0
end

function draw_player()
    spr(p.sprite,p.x*8,p.y*8)
end

function move_player()
    newx=p.x
    newy=p.y
    
    if (btnp(⬅️)) newx-=1
    if (btnp(➡️)) newx+=1
    if (btnp(⬆️)) newy-=1
    if (btnp(⬇️)) newy+=1
    
    interact(newx,newy)
    
    if (can_move(newx,newy)) then
    p.x=mid(0,newx,127)
    p.y=mid(0,newy,63)
    else
    sfx(0)
    end
end

function interact(x,y)
    if (is_tile(key,x,y)) then
    	get_key(x,y)
    elseif (is_tile(door,x,y) and p.keys>0) then
    	open_door(x,y)
    elseif (is_tile(chest,x,y)) and p.keys>0 then
        open_chest(x,y)
        p.gold=100
    end
end
-->8
--inventory

function show_inventory()
	invx=mapx*8+40
	invy=mapy*8+8
	
	rectfill(invx,invy,invx+56,invy+56,0)
	rect(invx,invy,invx+56,invy+56,7)
	print(" inventory",invx+7,invy+4,7)
	print("keys: "..p.keys,invx+12,invy+14,9)
    print("gold: "..p.gold,invx+12,invy+28,9)
end
-->8
-- animations

function toggle_tiles()
    for x=mapx,mapx+15 do
        for y=mapy,mapy+15 do
            if (is_tile(anim1,x,y)) then
                swap_tile(x,y)
                sfx(3)
            elseif (is_tile(anim2,x,y)) then
                unswap_tile(x,y)
                sfx(3)
            end
        end
    end
end
-->8
-- win/lose

function check_win_lose()
    if is_tile(win, p.x, p.y) then
        if p.keys >= 1 then
            state = "win"
        else
            state = "lose"
        end
    elseif is_tile(lose, p.x, p.y) then
        state = "lose"
    end
end

function draw_win_lose()
	camera()
	
	if state == "win" then
		print("★ you win! ★",37,64,7)
	else
		print("game over.",38,64,7)
	end
	
	print("press ❎ to play again.",20,72,5)
end
-->8
-- entities

function make_enemy()
    e={}
    e.x=6
    e.y=6
    e.sprite=2
    e.keys=0
    e.gold=0
end

function draw_enemy()
    spr(e.sprite,e.x*8,e.y*8)
end
