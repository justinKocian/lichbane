-- INTERACTION
tile_handlers = {}

function setup_tile_handlers()
    tile_handlers = {
        key = get_key,
        door_locked = open_locked_door,
        door_unlocked = open_unlocked_door,
        chest_locked = open_locked_chest,
        chest_unlocked = open_unlocked_chest,
        sign = read_sign,
        win = trigger_win
    }    
end

function interact(x, y)
    local t = tile_type_at(x, y)
    if tile_handlers[t] then
        tile_handlers[t](x, y)
    else
        check_npc_interact(x, y)
    end
end

function get_key(x, y)
    player.keys += 1
    swap_tile(x, y)
    clear_tile_type(x, y)
    play_sound(1)
end

function open_locked_door(x, y)
    if player.keys > 0 then
        player.keys -= 1
        swap_tile(x, y)
        clear_tile_type(x, y)
        play_sound(2)
    else
        play_sound(1)
    end
end

function open_unlocked_door(x, y)
    swap_tile(x, y)
    clear_tile_type(x, y)
    play_sound(2)
end

function open_locked_chest(x, y)
    if player.keys > 0 then
        player.keys -= 1
        swap_tile(x, y)
        clear_tile_type(x, y)
        player.gold += 100 -- or whatever reward
        play_sound(1)
    else
        play_sound(1)
    end
end

function open_unlocked_chest(x, y)
    swap_tile(x, y)
    clear_tile_type(x, y)
    player.gold += 100 -- or whatever reward
    play_sound(1)
end

function read_sign(x, y)
    change_state(game_states.dialogue)
    dtb_disp("an old, weathered sign.\n(more signs coming soon!)")
end

function trigger_win(x, y)
    win_flag = (player.keys >= 1)
    change_state(game_states.gameover)
end
